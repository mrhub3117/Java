import java.util.Scanner;

public class Main {
    
    
    public static <T, U, V, W> void displayInputs(T input1, U input2, V input3, W input4) {
        System.out.println("Input 1: " + input1);
        System.out.println("Input 2: " + input2);
        System.out.println("Input 3: " + input3);
        System.out.println("Input 4: " + input4);
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        
        System.out.print("Enter a String: ");
        String input1 = scanner.nextLine();

        System.out.print("Enter an Integer: ");
        int input2 = scanner.nextInt();

        System.out.print("Enter a Double: ");
        double input3 = scanner.nextDouble();

        System.out.print("Enter a Boolean: ");
        boolean input4 = scanner.nextBoolean();

        
        displayInputs(input1, input2, input3, input4);

        scanner.close();
    }
}
